    <!-- http://purl.obolibrary.org/obo/OMP_0000000 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0000000">
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The observable manifestation of the genotype of a microbial cell or group of cells in an environment.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">microbial phenotype</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0005002 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0005002">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0007192"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A growth phenotype where the trait in question is the range of temperatures where growth of the microbe can occur.</obo:IAO_0000115>
        <oboInOwl:hasNarrowSynonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">temperature requirement for growth</oboInOwl:hasNarrowSynonym>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">temperature growth range phenotype</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0005003 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0005003">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0005002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A temperature growth range phenotype where optimal growth occurs between 85 C and 113 C.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hyperthermophilic growth</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0005004 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0005004">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0005002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A temperature growth range phenotype where growth can occur at 55 C or higher and optimal growth occurs between 55 C and 65 C.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">thermophilic growth</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0005005 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0005005">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0005002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A temperature growth range phenotype where optimal growth occurs between 20 C and 45 C.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mesophilic growth</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0005006 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0005006">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0005002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A temperature growth range phenotype where growth of the occurs from 0 to 7 degrees C with optimum growth at 20 to 30 degrees C. The maximum temperature of growth is around 35 degrees C.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">psychrotrophic growth</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0005007 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0005007">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0005002"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A temperature growth range phenotype where growth can occur at 0 degrees C and optimal growth occurs at temperatures of 15 degrees C or lower.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">psychrophilic growth</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0007006 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0007006">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0000000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A microbial phenotype where the phenotype is observed at the level of a microbial population.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">population phenotype</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0007167 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0007167">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0007006"/>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0007194"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A microbial phenotype where the trait in question is the increase in the number of individuals in a microbial population as the result of sexual or asexual reproduction.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">population growth phenotype</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0007192 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0007192">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0007167"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A growth phenotype related to non-nutrient environmental factors and the requirements for microbial growth.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">growth range phenotype</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMP_0007194 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMP_0007194">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMP_0000000"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A microbial phenotype that affects a biological process.</obo:IAO_0000115>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">biological process phenotype</rdfs:label>
    </owl:Class>
